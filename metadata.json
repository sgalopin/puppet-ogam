{
  "name": "puppet-ogam",
  "version": "1.0.0",
  "author": "sgalopin",
  "summary": "Installs, configures, and manages OGAM virtual hosts, web services, and modules.",
  "license": "AGPL-v3",
  "source": "http://gitlab.dockerforge.ign.fr/puppet/puppet-ogam",
  "project_page": "http://gitlab.dockerforge.ign.fr/puppet/puppet-ogam",
  "issues_url": "http://gitlab.dockerforge.ign.fr/puppet/puppet-ogam/issues",
  "dependencies": [
    {"name": "puppetlabs-stdlib","version_requirement": ">= 4.24.0"},
    {"name":"puppetlabs-concat","version_requirement":">= 4.1.1"},
    {"name":"puppet-archive","version_requirement":">= 1.3.0"},
    {"name":"puppetlabs-apt","version_requirement":">= 4.5.0"},
    {"name":"puppetlabs-apache","version_requirement":">= 2.3.0"},
    {"name":"puppetlabs-vcsrepo","version_requirement":">= 2.2.0"},
    {"name":"puppetlabs-java","version_requirement":">= 2.3.0"},
    {"name":"puppetlabs-tomcat","version_requirement":">= 2.1.0"},
    {"name":"puppetlabs-postgresql","version_requirement":">= 5.2.1"}
  ],
  "data_provider": null,
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04",
        "16.04"
      ]
    }
  ],
   "requirements": [
     {
       "name": "puppet",
       "version_requirement": ">= 3.0.0 < 5.0.0"
     }
   ],
   "description": "Module for OGAM configuration"
}

